{
    // IntelliSense を使用して利用可能な属性を学べます。
    // 既存の属性の説明をホバーして表示します。
    // 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "API: Post Favorites",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/create_favorites.py",
            "console": "integratedTerminal",
            "env": {
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "API: Local",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "env": {
                "IS_DEBUG": "true",
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        },
        {
            "name": "API: Dev-Twitter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "env": {
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        }
        {
            "name": "API: Dev-CloudSQL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--settings=twitterclient.settings.development"
            ],
            "django": true,
            "env": {
                "IS_DEBUG": "true",
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        },
        {
            "name": "API: Dev-CloudSQL Dev-Twitter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--settings=twitterclient.settings.development"
            ],
            "django": true,
            "env": {
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        }
    ]
}