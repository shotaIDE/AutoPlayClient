{
    // IntelliSense を使用して利用可能な属性を学べます。
    // 既存の属性の説明をホバーして表示します。
    // 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Agent: Fetch Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/fetch_debug.py",
            "console": "integratedTerminal",
            "env": {
                "IS_DEBUG": "true",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Agent: Fetch Production",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/fetch_debug.py",
            "console": "integratedTerminal",
            "env": {
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Agent: Post Favorites",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/post_favorite_debug.py",
            "console": "integratedTerminal",
            "env": {
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Django: Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "env": {
                "IS_DEBUG": "true",
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        },
        {
            "name": "Django: Production",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "env": {
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/serviceAccountKey-Dev.json",
                "CACHE_PATH": "${workspaceFolder}/cache.json",
                "VIDEO_LIST_PATH": "${workspaceFolder}/src/video_list.json"
            },
        }
    ]
}